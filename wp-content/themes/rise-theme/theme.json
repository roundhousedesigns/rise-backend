{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--custom--font-family--body)",
			"lineHeight": "1.5",
			"fontSize": "18px"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--custom--font-family--heading)",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--custom--font-family--heading)",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--custom--font-family--heading)",
					"lineHeight": "1.1"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--custom--font-family--heading)",
					"lineHeight": "1.3"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--custom--font-family--heading)",
					"lineHeight": "1.4"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--custom--font-family--heading)",
					"lineHeight": "1.4"
				}
			}
		},
		"blocks": {
			"core/buttons": {
				"spacing": {
					"margin": {
						"top": "1em",
						"bottom": "1em"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--radius)"
				}
			}
		}
	},
	"settings": {
		"layout": {
			"contentSize": "840px",
			"wideSize": "1100px"
		},
		"useRootPaddingAwareAlignments": true,
		"color": {
			"custom": false,
			"palette": [
				{
					"name": "Yellow",
					"slug": "yellow",
					"color": "#C9DB58"
				},
				{
					"name": "Green",
					"slug": "green",
					"color": "#274F42"
				},
				{
					"name": "Red",
					"slug": "red",
					"color": "#962F34"
				},
				{
					"name": "Orange",
					"slug": "orange",
					"color": "#F15A29"
				},
				{
					"name": "Blue",
					"slug": "blue",
					"color": "#00AEEF"
				},
				{
					"name": "Light Blue",
					"slug": "light-blue",
					"color": "#33BEFF"
				},
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#FFFFFF"
				},
				{
					"name": "Dark Gray",
					"slug": "dark-gray",
					"color": "#1A2231"
				},
				{
					"name": "Background (Dark)",
					"slug": "bg-dark",
					"color": "#171923"
				},
				{
					"name": "Background (Light)",
					"slug": "bg-light",
					"color": "#F7FAFC"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Victor Serif\", serif",
					"name": "Victor Serif",
					"slug": "victor-serif"
				},
				{
					"fontFamily": "\"Stabil Grotesk\", sans-serif",
					"name": "Stabil Grotesk",
					"slug": "stabil-grotesk"
				}
			],
			"fluid": {
				"minFontSize": "14px",
				"maxViewportWidth": "1000px",
				"minViewportWidth": "320px"
			},
			"fontSizes": [
				{
					"size": "14px",
					"name": "Small",
					"slug": "small"
				},
				{
					"size": "20px",
					"name": "Medium",
					"slug": "medium",
					"fluid": {
						"min": "18px",
						"max": "22px"
					}
				},
				{
					"size": "24px",
					"name": "Large",
					"slug": "large",
					"fluid": {
						"min": "24px",
						"max": "32px"
					}
				},
				{
					"size": "28px",
					"name": "X-Large",
					"slug": "x-large",
					"fluid": {
						"min": "28px",
						"max": "36px"
					}
				},
				{
					"size": "36px",
					"name": "XX-Large",
					"slug": "xx-large",
					"fluid": {
						"min": "32px",
						"max": "50px"
					}
				}
			]
		},
		"custom": {
			"fontFamily": {
				"heading": "\"Stabil Grotesk\", sans-serif",
				"body": "\"Victor Serif\", serif"
			},
			"borderRadius": {
				"radius": "0.375rem"
			}
		},
		"blocks": {
			"core/cover": {
				"spacing": {
					"margin": true,
					"padding": true
				}
			},
			"core/group": {
				"spacing": {
					"margin": true,
					"padding": true
				}
			},
			"core/columns": {
				"spacing": {
					"margin": true,
					"padding": true
				}
			}
		}
	}
}